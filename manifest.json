{
    "name": "Stahpit",
    "version": "1.0",
    "description": "Anting's anti-distraction utilities",
    "manifest_version": 2,
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}
